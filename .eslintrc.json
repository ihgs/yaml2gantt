{
    "extends": ["eslint:recommended", "plugin:node/recommended"],
    "plugins": ["node"],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {"browser": true, "node": true},
    "globals": {},
    "rules": {
      "node/no-unsupported-features": [2, {"version": 5}],
      "semi": "error"
    }
}
