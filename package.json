{
  "name": "yaml2gantt",
  "version": "0.6.1",
  "description": "A client tool to output Ganttchart SVG from yaml.",
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true,
  "bin": "cli.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint  --write",
      "eslint",
      "git add"
    ]
  },
  "keywords": [
    "ganttchart"
  ],
  "author": "ihgs",
  "license": "MIT",
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^2.9.0",
    "d3": "^5.9.2",
    "deasync": "^0.1.12",
    "js-yaml": "^3.6.1",
    "jsdom": "^14.0.0",
    "moment": "^2.13.0",
    "simple-git": "^1.92.0",
    "svg2png": "^4.1.1",
    "sync-request": "^6.1.0"
  },
  "devDependencies": {
    "eslint-plugin-node": "^8.0.0",
    "gulp": "4.0.0",
    "gulp-clang-format": "^1.0.23",
    "gulp-eslint": "^5.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.0.0",
    "prettier": "^1.11.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ihgs/yaml2gantt.git"
  }
}
